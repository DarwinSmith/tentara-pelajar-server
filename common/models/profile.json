{
  "name": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "userId": {
      "type": "string",
      "required": true
    },
    "schoolId": {
      "type": "number"
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "experience": {
      "type": "string"
    },
    "activity": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "skills": {
      "type": "hasMany",
      "model": "skill",
      "foreignKey": "profileId",
      "through": "profile_skill"
    },
    "posts": {
      "type": "hasMany",
      "model": "post",
      "foreignKey": ""
    },
    "notifications": {
      "type": "hasMany",
      "model": "notification",
      "foreignKey": ""
    },
    "friends": {
      "type": "hasMany",
      "model": "friend",
      "foreignKey": "profileId",
      "through": "friend"
    },
    "personality_endorsements": {
      "type": "hasMany",
      "model": "personalities",
      "foreignKey": "",
      "through": "personality_endorsements"
    }
  },
  "acls": [],
  "methods": {}
}
