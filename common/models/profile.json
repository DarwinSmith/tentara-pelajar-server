{
  "name": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userId": {
      "type": "string",
      "required": true
    },
    "schoolId": {
      "type": "number"
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "experience": {
      "type": "string"
    },
    "activity": {
      "type": "string"
    }
  },
  "mixins": {
     "TimeStamp" : true
  },
  "validations": [],
  "relations": {
    "skills": {
      "type": "hasMany",
      "model": "skill",
      "foreignKey": "profileId",
      "through": "profile_skill"
    }
  },
  "acls": [],
  "methods": {}
}
