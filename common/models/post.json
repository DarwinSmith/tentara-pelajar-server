{
  "name": "post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "profileId": {
      "type": "number",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "profile": {
      "type": "belongsTo",
      "model": "profile",
      "foreignKey": "profileId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": ""
    },
    "reactions": {
      "type": "hasMany",
      "model": "reaction",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
